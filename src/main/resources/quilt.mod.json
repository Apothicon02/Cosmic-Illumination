{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "Stuff",
      "contributors": {
        "Author": "Apothicon"
      },

      "contact": {
        "homepage": "https://github.com/Apothicon02/Terraflat",
        "issues": "https://discord.gg/HHqKa8vTAN",
        "sources": "https://github.com/Apothicon02/Terraflat"
      },

      "icon": "assets/terraflat/icon.png"
    },

    "entrypoints": {
      "init": "com.apothicon.terraflat.Terraflat"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": "*"
      },
      {
        "id": "cosmicreach",
        "versions": "*"
      }
    ]
  },

  "mixin": "terraflat.mixins.json"
}
